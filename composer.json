{
    "name": "alexfn/nano-service",
    "description": "This is my package nano-service",
    "keywords": [
        "AlexFN",
        "nano-service"
    ],
    "homepage": "https://github.com/alexfn/nano-service",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander O.",
            "email": "falkonseo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.1",
        "php-amqplib/php-amqplib": "^3.5",
        "ramsey/uuid": "^3 || ^4",
        "spatie/crypto": "^2.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "laravel/pint": "^1.2",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "AlexFN\\NanoService\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlexFN\\NanoService\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
