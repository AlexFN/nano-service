{
    "name": "alexfn/nano-service",
    "description": "This is my package nano-service",
    "keywords": [
        "AlexFN",
        "nano-service"
    ],
    "homepage": "https://github.com/alexfn/nano-service",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander O.",
            "email": "falkonseo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "league/statsd": "^2.0",
        "myclabs/php-enum": "^1.8",
        "php-amqplib/php-amqplib": "^3.5",
        "ramsey/uuid": "^3 || ^4",
        "spatie/crypto": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "AlexFN\\NanoService\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlexFN\\NanoService\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
